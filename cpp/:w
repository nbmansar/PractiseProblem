#include<stdio.h>

int main(){
int arr[10]={1,3,0,2,5,0,4,1,0,1};
int n = sizeof(arr)/sizeof(arr[0]);
int temp=0,count=0;
for(int i=0;i<n;i++){
	if(arr[i] == 0){
		printf("%d\n",count++);
		arr[i] = temp;
		arr[i] = arr[i+1];
		arr[i+1] = temp;
	}
}
for(int i=0;i<n;i++){
	printf("%d\n",arr[i]);
}
}
